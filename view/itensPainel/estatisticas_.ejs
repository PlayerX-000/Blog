<body>
   
    <header class="sticky-top">
        <!--Menu antes do colappso-->
            <div class="pos-f-t">
          <nav class="navbar navbar-expand-lg navbar-dark bg-menu">
              <div class="container-fluid">
                <a id="logoSite" class="navbar-brand" style="font-size: calc(31px + (48 - 31) * ((100vw - 400px) / (2600 - 400)));" href="/"><%= marca%></a>
                    <a id="btnMenu" class="navbar-toggler menu-toggle"><i class="fas fa-bars"></i></a>
                <div class="collapse navbar-collapse menu-s" id="navbarSupportedContent">
                  <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="dashboard-nav-item nav-link" aria-current="page" href="/">Inicio</a>
                    </li>
                    <li class="nav-item">
                        <a class="dashboard-nav-item nav-link active" href="/painel" aria-current="page">Dashboard</a>
                    </li>
                  </ul>
                  <a class="menu-toggle"><i class="fas fa-bars"></i></a>
                </div>
              </div>
            </nav>
            </div>
      </header>


      <main id="principal" style="margin-bottom: 50%;"> 
      <div class='dashboard dashboard-compact'>
        <div class="dashboard-nav" id="menuLateral">
            <nav class="dashboard-nav-list">
               
                <a href="/painel" id="Apresentacao" class="dashboard-nav-item">
                <i class="fas fa-home"></i>Apresentação</a>

                <a id="dashboardItem" style="color: azure;" class="dashboard-nav-item">
                <i class="fas fa-tachometer-alt"></i>Criação de Postagens</a>

                <a id="configuracoesItem" style="color: azure;" class="dashboard-nav-item">
                <i class="fas fa-cogs"></i> Configurações </a>

                <a href="/estatisticas" id="estatisticasItem" style="color: azure;" class="dashboard-nav-item">
                <i class="fa fa-bar-chart" aria-hidden="true"></i>Estatisticas </a>

                <a id="perfilItem" style="color: azure;" class="dashboard-nav-item">
                <i class="fas fa-user"></i> Perfil</a>

              <div class="nav-item-divider"></div>
              <a id="sairContaItem" style="color: azure;" class="dashboard-nav-item"><i class="fas fa-sign-out-alt"></i>Sair</a>
            </nav>
        </div>

        <div class='dashboard-app' id="conteudo" style="height: 100%;">       
            <div class="card-m">
                <div class="card-header">
                    <h2>Estatisticas</h2>
                </div>
                <div class="card-body">
                <p class="w-100">Usuarios Conectados: <span id="userOn" style="color: rgb(0, 169, 11);">0</span></p>
                
                <hr>

                  <div>
                    <div id="graficoEstatisticaAcesso" class="borda">
                        <h3>Acessos na Pagina (GRAFICO)</h3>
                    <canvas id="EstatisticaChart"></canvas>
                    </div>              
                    <div id="graficoRankPost" class="borda">
                        <h3>Visualização de Postagens (GRAFICO)</h3>
                    <canvas id="PostChart"></canvas> 
                    </div>

                  </div>
                  
            </div>
        </div>
            </div>
            <div id="resize" style="position: initial;"></div>
            </div>
        </div>
    </div>
</main>
